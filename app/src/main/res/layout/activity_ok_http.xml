<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.OkHttpActivity"
    tools:ignore="MissingDefaultResource">

    <include layout="@layout/title_easy"></include>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="50dp"
        android:orientation="vertical">

        <Button
            android:id="@+id/okhttputils"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="OkHttpUtils"></Button>

        <Button
            android:id="@+id/okgo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="OkGo"></Button>

        <Button
            android:id="@+id/okhttp3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="OkHttp3"></Button>

        <Button
            android:id="@+id/retrofit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Retrofit"></Button>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>